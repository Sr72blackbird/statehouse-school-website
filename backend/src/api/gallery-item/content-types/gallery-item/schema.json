{
  "kind": "collectionType",
  "collectionName": "gallery_items",
  "info": {
    "singularName": "gallery-item",
    "pluralName": "gallery-items",
    "displayName": "Gallery Item"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "gallery_album": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::gallery-album.gallery-album",
      "inversedBy": "gallery_items"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "caption": {
      "type": "string"
    },
    "order": {
      "type": "integer"
    }
  }
}
