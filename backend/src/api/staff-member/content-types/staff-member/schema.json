{
  "kind": "collectionType",
  "collectionName": "staff_members",
  "info": {
    "singularName": "staff-member",
    "pluralName": "staff-members",
    "displayName": "Staff Member"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "full_name": {
      "type": "string",
      "required": true
    },
    "job_title": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::staff-category.staff-category",
      "inversedBy": "staff_members"
    },
    "photo": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "biography": {
      "type": "blocks"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "order": {
      "type": "integer"
    }
  }
}
